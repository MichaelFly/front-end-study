  "templateInfo": {
    "name": "react-NutUI",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:font": "npx iconfont-taro",
    "build:h5:zh": "taro build --type h5",
    "build:h5:ko": "taro build --type h5 --mode ko",
    "dev:h5": "npm run build:h5:zh -- --watch",
    "dev:h5:ko": "npm run build:h5:ko -- --watch",
    "extract": "formatjs extract",
    "intl:zh": "npm run extract -- \"src/**/*.{ts,tsx}\" --ignore=\"**/*.d.ts\" --out-file src/components/lang/locales/zh-CN/index.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
